{
  "lock": false,
  "name": "@fartlabs/css",
  "version": "0.0.11",
  "exports": {
    ".": "./components/mod.ts",
    "./border-tube": "./components/border-tube.tsx",
    "./button": "./components/button.tsx",
    "./button-group": "./components/button-group.tsx",
    "./button-link": "./components/button-link.tsx",
    "./header": "./components/header.tsx",
    "./horizontal-rule": "./components/horizontal-rule.tsx",
    "./link": "./components/link.tsx",
    "./section": "./components/section.tsx",
    "./shake": "./components/shake.tsx",
    "./sparkle": "./components/sparkle.tsx",
    "./text-gradient": "./components/text-gradient.tsx",
    "./text-special": "./components/text-special.tsx"
  },
  "tasks": {
    "precommit": {
      "description": "Run the precommit tasks",
      "dependencies": ["check", "lint", "generate"]
    },
    "dev": {
      "command": "deno serve -A --watch main.ts",
      "description": "Run the dev server",
      "dependencies": ["generate"]
    },
    "lint": "deno lint && deno doc --lint  components/**/*.ts",
    "check": "deno check **/*.ts",
    "publish:dry": "deno publish --dry-run --allow-dirty",
    "outdated": "deno update --latest",
    "generate": "deno --allow-write --allow-read codegen/generate.ts && deno fmt"
  },
  "imports": {
    "@fartlabs/htx": "jsr:@fartlabs/htx@^0.0.11",
    "@std/http": "jsr:@std/http@^1.0.21",
    "alea": "npm:alea@^1.0.1",
    "simplex-noise": "npm:simplex-noise@^4.0.3",
    "turndown": "npm:turndown@^7.2.2"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@fartlabs/htx"
  }
}
